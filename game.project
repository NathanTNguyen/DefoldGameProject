[project]
title = Dungeon Escape
version = 1.0
dependencies = https://github.com/rgrams/rendercam/archive/v1.0.1.zip

[bootstrap]
main_collection = /main/main.collectionc
render = /rendercam/rendercam.renderc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 768
height = 432
fullscreen = 0
update_frequency = 60
vsync = 0

[physics]
scale = 0.02
gravity_y = 0.0
gravity_x = 0.0

[script]
shared_state = 1

